{
  "presets": [
    "es2015",
    "stage-2",
    "react"
  ],
  "env" : {
    "production" : {
      "presets": [
        "es2015",
        "stage-2",
        "react"
      ],
      "plugins" : [
        "transform-react-constant-elements",
        "transform-react-inline-elements",
        "transform-react-remove-prop-types",
        "transform-react-remove-prop-types",
        "transform-react-pure-class-to-function",
        "transform-runtime",
      ]
    },
    "development": {
      "presets": [
        "es2015",
        "stage-2",
        "react"
      ],
      "plugins" : [
        "transform-runtime"
      ]
    }
  }
}
